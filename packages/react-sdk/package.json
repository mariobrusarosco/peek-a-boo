{
  "name": "@peek-a-boo/react-sdk",
  "version": "0.0.1",
  "description": "React SDK for Peek-a-boo feature flags",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "dist/index.d.ts",
      "import": "dist/index.esm.js",
      "require": "dist/index.cjs.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "type:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@vitest/ui": "^1.0.0",
    "jsdom": "^23.0.0",
    "msw": "^2.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "feature-flags",
    "react",
    "hooks",
    "feature-toggles",
    "peek-a-boo"
  ],
  "author": "Peek-a-boo Team",
  "license": "MIT"
}
